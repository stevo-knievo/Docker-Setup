    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutterapp",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "API_KEY=123456789",
                "--dart-define",
                "PASSWORD=MY_PASSWORD"
            ]
        },
        {
            "name": "flutterapp (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define",
                "API_KEY=123456789",
                "--dart-define",
                "PASSWORD=MY_PASSWORD"
            ]
        },
        {
            "name": "flutterapp (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define",
                "API_KEY=123456789",
                "--dart-define",
                "PASSWORD=MY_PASSWORD"
            ]
        }
    ]
}
